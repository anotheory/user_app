<?xml version="1.0" encoding="utf-8" ?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

	<changeSet id="create_table-healthcheck" author="anotheory">
		<createTable tableName="healthcheck" schemaName="user_management">
			<column name="id" type="INT" autoIncrement="true">
				<constraints primaryKey="true"></constraints>
			</column>
			<column name="message" type="VARCHAR(255)"></column>
		</createTable>

		<insert tableName="healthcheck" schemaName="user_management">
			<column name="message" value="I'm fine, thank you." />
		</insert>
	</changeSet>

</databaseChangeLog>